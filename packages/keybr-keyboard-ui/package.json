{
  "private": true,
  "name": "@keybr/keyboard-ui",
  "version": "0.0.0",
  "type": "module",
  "main": "lib/index.ts",
  "types": ".types/index.d.ts",
  "dependencies": {
    "@keybr/intl": "*",
    "@keybr/keyboard": "*",
    "@keybr/unicode": "*",
    "@keybr/widget": "*"
  },
  "devDependencies": {
    "@keybr/test-env-time": "*"
  },
  "scripts": {
    "clean": "rm -fr .types",
    "compile": "tsc",
    "test": "ava"
  },
  "ava": {
    "files": [
      "lib/**/*.test.*"
    ],
    "extensions": {
      "ts": "module",
      "tsx": "module"
    },
    "nodeArguments": [
      "--import=@keybr/tsl",
      "--import=@keybr/test-env-browser",
      "--import=@keybr/test-env-bundler"
    ]
  }
}
