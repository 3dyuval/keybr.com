{
  "private": true,
  "name": "@keybr/generators",
  "version": "0.0.0",
  "type": "module",
  "main": "lib/index.ts",
  "dependencies": {
    "@keybr/keyboard": "*",
    "@keybr/phonetic-model": "*",
    "@keybr/unicode": "*",
    "@types/commonmark": "^0.27.9",
    "chalk": "^5.3.0",
    "commonmark": "^0.31.2"
  },
  "devDependencies": {},
  "scripts": {
    "clean": "rm -fr .types",
    "compile": "tsc",
    "test": "ava",
    "generate-books": "node --import @keybr/tsl ./lib/generate-books.ts",
    "generate-languages": "node --import @keybr/tsl ./lib/generate-languages.ts",
    "generate-layouts": "node --import @keybr/tsl ./lib/generate-layouts.ts"
  },
  "ava": {
    "files": [
      "lib/**/*.test.*"
    ],
    "extensions": {
      "ts": "module",
      "tsx": "module"
    },
    "nodeArguments": [
      "--import=@keybr/tsl"
    ]
  }
}
